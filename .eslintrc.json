{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "naver",
    "prettier"
  ],
  "rules": {
    "no-console": 1,
    "no-alert": 0,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "consistent-return": 0
  },
  "root": true
}
